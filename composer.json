{
  "name": "mrtawil/adminro",
  "description": "Adminro, makes Laravel easier.",
  "keywords": [
    "laravel",
    "adminro"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abed Tawil",
      "email": "aboditawil@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "laravel/ui": "^3.3",
    "livewire/livewire": "^2.10",
    "laravel/framework": "^8.0",
    "yajra/laravel-datatables": "^1.5",
    "yajra/laravel-datatables-buttons": "^4.0",
    "yajra/laravel-datatables-html": "^4.0",
    "yajra/laravel-datatables-oracle": "^9.18"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Adminro\\": "/src"
    },
    "files": [
      "/src/Helpers/general.php",
      "/src/Helpers/dataTable.php",
      "/src/Helpers/forms.php",
      "/src/Helpers/model.php",
      "/src/Helpers/request.php",
      "/src/Helpers/select.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adminro\\Providers\\AdminroServiceProvider"
      ]
    }
  }
}